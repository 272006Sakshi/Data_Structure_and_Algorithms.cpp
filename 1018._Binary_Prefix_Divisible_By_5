class Solution {
public:
    vector<bool> prefixesDivBy5(vector<int>& nums) {
        vector<bool> res;
        // brute force O(n^2)
        // for (int i = 0; i < nums.size(); i++) {
        //     long long num = 0;
            
        //     // Convert nums[0..i] into a decimal number
        //     for (int j = 0; j <= i; j++) {
        //         num = num * 2 + nums[j];  
        //     }
            
        //     // Check divisible by 5
        //     if (num % 5 == 0) 
        //         res.push_back(true);
        //     else 
        //         res.push_back(false);
        // }

        int num = 0;
        for (int i = 0; i<nums.size(); i++) {
           num = (num * 2 + nums[i]) % 5;
           if(num == 0) res.push_back(true);
           else{
              res.push_back(false);
           }
        
        }

        
        return res;
    }
};
