class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        // O(n)
        int n = nums.size();
        // for(int i = 0; i<n; i++){
        //     if(nums[i]>= target){
        //         return i;
        //     }
        // }
        // return n;

       // O(logn)
        int low = 0, high = n-1;
        while(low<=high){
            int mid = low+(high-low)/2;
            if(nums[mid] == target) return mid;
            else if(nums[mid]<target){
                low = mid+1;
            }
            else{
                high = mid-1;
            }
        }  
        return low;  

    }
};
