class Solution {
public:
    vector<int> nextGreaterElement(vector<int>& nums1, vector<int>& nums2) {
       // T.C : O(n + m)
      // S.C : O(n)  --> map + stack (dabba)
        unordered_map<int, int> nxtgre;
        stack<int> dabba;
        for(int i = 0; i<nums2.size(); i++){
            while(!dabba.empty() && dabba.top()<nums2[i]){
                nxtgre[dabba.top()] = nums2[i];
                dabba.pop();
            }
            dabba.push(nums2[i]);
        }
        while(!dabba.empty()){
                nxtgre[dabba.top()] = -1;
                dabba.pop();
        }

        vector<int> res;
        for(int j = 0; j<nums1.size(); j++){
            res.push_back(nxtgre[nums1[j]]);
        }
        return res;
    }
};
