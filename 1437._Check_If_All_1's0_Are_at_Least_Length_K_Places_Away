class Solution {
public:
    bool kLengthApart(vector<int>& nums, int k) {
        //T.C: O(n) 
        //S.C: O(1)
        int n = nums.size();
        int prPos = -1;

        for(int i = 0; i<n; i++){
            if(nums[i] == 1){
                if(prPos != -1 && i-prPos-1<k) return false;
                prPos = i;
            }
        }
        return true;

    }
};
