class Solution {
public:
    bool canJump(vector<int>& nums) {
        int n = nums.size();
        int jump = n-1;
        if(n==1) return true;
        // reverse Analogy
        for(int i = n-2; i>=0; i--){
            if(i+nums[i]>=jump){
                if(i==0)return true;
                jump = i;
            }
        }
        return false;
    }
};
