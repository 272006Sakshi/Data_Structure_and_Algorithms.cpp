class Solution {
public:
    int countTriples(int n) {
        int res = 0;
        for(int a = 1; a<=n; a++){
            for(int b = 1; b<=n; b++){
                int sqSum = a*a + b*b;
                int c = sqrt(sqSum);

                if(c<=n && c*c == sqSum){
                    res+=1;
                }
            }
        }
        return res;
    }
};
