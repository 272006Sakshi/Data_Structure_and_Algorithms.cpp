class Solution {
public:
    vector<int> nextGreaterElements(vector<int>& nums) {
        int n = nums.size();
        vector<int> res(n, -1);
        stack<int> dabba;  

        for (int i = 2 * n - 1; i >= 0; --i) {
            int idx = i % n;
            while (!dabba.empty() && nums[dabba.top()] <= nums[idx]) {
                dabba.pop();
            }
            if (i < n && !dabba.empty()) {
                res[idx] = nums[dabba.top()];
            }
            dabba.push(idx);
        }
        return res;
        
    }
};
