class Solution {
public:
    int sumOfUnique(vector<int>& nums) {
        int n = nums.size();
        int sum = 0;
        
        // unordered_map<int, int> mp;
        int freq[101] = {0}; // fix size S.C: Constant, T.C: O(n)
        for(int i = 0; i<n; i++){
            freq[nums[i]]++;

        }
        for(int j  = 0; j<=100; j++){
            if(freq[j] == 1){
                sum+=j;
            }
        }
        return sum;
        
    }
};
