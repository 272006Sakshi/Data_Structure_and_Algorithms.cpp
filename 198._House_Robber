class Solution {
public:
    int rob(vector<int>& nums) {
        int n = nums.size();

        if(n==0) return 0;
        if(n == 1) return nums[0];

        vector<int> Genius(n);
        Genius[0] = nums[0];
        Genius[1] = max(nums[0], nums[1]);

        for(int i = 2; i<n; i++){
            Genius[i] = max(Genius[i-1], nums[i]+Genius[i-2]);
        }

        return Genius[n-1];
    }
};
